generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Pricing {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  meta  Meta
  limit Int
}

type Meta {
  reference    String @default(uuid())
  name         String
  quantity     Int
  unit_price   Int
  total_amount Int
  tax_rate     Int
}
